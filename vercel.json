{
  "version": 2,
  "rewrites": [
    {
      "source": "/app/:word",
      "destination": "/app/[word]"
    },
    {
      "source": "/dict/:word",
      "destination": "/dict/[word]"
    },
    {
      "source": "/dictapp/:word",
      "destination": "/dictapp/[word]"
    }
  ],
  "trailingSlash": false
}
